{
  "name": "clubexpress-sdk",
  "version": "0.1.0",
  "description": "A TypeScript SDK for interacting with ClubExpress platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "test:auth": "ts-node scripts/test-auth.ts",
    "test:courts": "ts-node scripts/test-courts.ts",
    "test:court-booking": "ts-node scripts/test-court-booking.ts",
    "cli:courts": "ts-node scripts/courts-cli.ts"
  },
  "keywords": [
    "clubexpress",
    "sdk",
    "api",
    "pickleball"
  ],
  "author": "Saddlebrooke Pickleball Club",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "chalk": "^5.4.1",
    "cheerio": "^1.0.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.2"
  }
}
